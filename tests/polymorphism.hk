type Point {
    x = 0;
    y = 0;

    do(x) => 17 + x;
};

type Point2 inherits Point {
    z = 0;

    do(x) => x + 1;
};

let a = new Point(3, 4) in let c=print(a.x), d=print(a.y), e=print(a.y), f=print(a.x) in 0;

let b = new Point(1, 1);

print(b.do(9));

let k = new Point2(1, 6, 9);

print(k.do(1));
print(k.x);
print(k.y);
print(k.z);
